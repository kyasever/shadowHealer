{
  "name": "hexgame-console",
  "description": "console-version for hex game, prepare for h5 version",
  "version": "1.0.0",
  "keywords": [
    "toolkit",
    "cli"
  ],
  "scripts": {
    "dev": "vite",
    "build-vue": "vue-tsc --noEmit && vite build",
    "publish-vue": "node ./build_vue.js",
    "localpublish-vue": "cd ./dist && python3 -m http.server 8080",
    "dev-node": "yarn tsc --build node_tsconfig.json --watch",
    "run-node": "node bin/node/index.js"
  },
  "files": [
    "lib",
    "bin"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "node-localstorage": "^2.2.1",
    "phaser": "^3.55.2",
    "vant": "3",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^16.7.13",
    "@types/webgl2": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "26.6.0",
    "jest-html-reporter": "^3.4.1",
    "prettier": "^1.19.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-style-import": "^1.4.0",
    "vue-tsc": "^0.3.0"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 0.12"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ]
}
